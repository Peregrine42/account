<h1>Categories</h1>
<table>
  <% @categories.each do |category| %>
    <tr>
      <td>
        <%= category.name %>
      </td>
      <td>
        <a href="/category/<%= category.id %>/edit">Edit</a>
      </td>
      <td>
        <a href="/category/<%= category.id %>/delete">Delete</a>
      </td>
    </tr>
  <% end %>
</table>
